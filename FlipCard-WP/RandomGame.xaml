<!--LayoutRoot is the root grid where all page content is placed-->
	<Grid x:Name="LayoutRoot" Background="Transparent" Margin="0,0,0,-1">

		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition x:Name="TopLine" Height="Auto"/>
			<RowDefinition Height="auto" MinHeight="480"/>
			<RowDefinition MaxHeight="130"/>
		</Grid.RowDefinitions>

		<!--TitlePanel contains the name of the application and page title-->

        <!--TitlePanel contains the name of the application and page title-->
		<TextBlock x:Name="ComboText" HorizontalAlignment="Left" TextWrapping="Wrap" Text="COMBO" VerticalAlignment="Top" RenderTransformOrigin="0.492,0.5" TextOptions.TextHintingMode="Animated" FontFamily="Segoe WP Black" Margin="0,143,0,0" Grid.Row="1" Grid.ColumnSpan="1" Width="480" Height="144" TextAlignment="Center" FontSize="93.333" Opacity="0" Canvas.ZIndex="-1" Grid.RowSpan="1">
			<TextBlock.RenderTransform>
				<CompositeTransform/>
			</TextBlock.RenderTransform>
		</TextBlock>

        <!--TitlePanel contains the name of the application and page title-->
		<StackPanel x:Name="TopPanel" Grid.Row="0" Margin="0,30,0,29" RenderTransformOrigin="0.5,0.5" Height="Auto">
			<StackPanel.RenderTransform>
				<CompositeTransform/>
			</StackPanel.RenderTransform>
			<StackPanel x:Name="RetryPanel" Margin="0,0,0,0" RenderTransformOrigin="0.5,0.5" Height="0">
				<StackPanel.RenderTransform>
					<CompositeTransform/>
				</StackPanel.RenderTransform>
				<TextBlock x:Name="Title_RetryPanel" Text="" Style="{StaticResource PhoneTextExtraLargeStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo"/>
				<TextBlock x:Name="Description_RetryPanel" Text="" Style="{StaticResource PhoneTextNormalStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo"/>
				<Grid Height="auto">
					<Grid.RowDefinitions>
						<RowDefinition Height="auto"/>
						<RowDefinition Height="100"/>
					</Grid.RowDefinitions>
					<Grid.ColumnDefinitions>
						<ColumnDefinition Width="2*"/>
						<ColumnDefinition Width="2*"/>
						<ColumnDefinition Width="auto"/>
					</Grid.ColumnDefinitions>
					<Button Grid.Column="0" x:Name="RetryButton_RetryPanel" Click="RetryButton_RetryPanel_Click" Content="{Binding LocalizedResources.Again_Question, Source={StaticResource LocalizedStrings}}"/>
					<Button Grid.Column="1" x:Name="CancelButton_RetryPanel" Click="CancelButton_RetryPanel_Click"  Content="{Binding LocalizedResources.Cancel, Source={StaticResource LocalizedStrings}}"/>
					<Button Grid.Column="2" x:Name="sharestatus" Click="sharestatus_Click" BorderThickness="0" RenderTransformOrigin="0.316,0.476">
						<Image Source="/Images/AppBar/share.png" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="None"/>
					</Button>
					<TextBlock Grid.Row="1" Grid.ColumnSpan="3" TextWrapping="Wrap" x:Name="EndMatchStatsBox" Foreground="#FFF1BB46" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo" Margin="12,5,12,0"/>
				</Grid>
			</StackPanel>
			<TextBlock x:Name="textBlock" Text="Random Match - FlipCard" Style="{StaticResource PhoneTextNormalStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo">
				<TextBlock.Projection>
					<PlaneProjection/>
				</TextBlock.Projection>
			</TextBlock>
<!-- <TextBlock Text="{Binding Path=LocalizedResources.ApplicationTitle, Source={StaticResource LocalizedStrings}}" Margin="9,-7,0,0" Style="{StaticResource PhoneTextExtraLargeStyle}"/>-->
			<Grid x:Name="TopGameBar" Margin="0,10,0,0" Height="auto">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="auto"/>
					<ColumnDefinition Width="auto"/>
					<ColumnDefinition Width="auto"/>
					<ColumnDefinition Width="5*"/>
				</Grid.ColumnDefinitions>
				<TextBlock Grid.Column="0" x:Name="ActualScoreYou" Foreground="#ff8d6e"  Style="{StaticResource PhoneTextExtraLargeStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo"/>
				<TextBlock x:Name="SepYouCPu" Grid.Column="1" Style="{StaticResource PhoneTextExtraLargeStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo">
					<Run Text="-"/>
				</TextBlock>
				<TextBlock Grid.Column="2" x:Name="ActualScoreCPU" Foreground="#6f99ff"  Style="{StaticResource PhoneTextExtraLargeStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo"/>
				<TextBlock Grid.Column="3" x:Name="ActualRow" Foreground="#FFF1BB46" TextAlignment="Right" Style="{StaticResource PhoneTextExtraLargeStyle}" FontFamily="/FlipCard-WP;component/Fonts/Fonts.zip#Alpaca Scarlett Demo"/>
			</Grid>
		</StackPanel>

